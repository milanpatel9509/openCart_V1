<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE project>

<suite name="Open Cart Test Suite" verbose="4" thread-count="4"
	parallel="tests">

	<listeners>
		<listener
			class-name="com.qa.opencart.listeners.ExtentReportListener" />
		<listener
			class-name="com.qa.opencart.listeners.TestAllureListener" />

		<listener
			class-name="com.qa.opencart.listeners.AnnotationTransformer" />

	</listeners>

	<test name="Open Cart Login Page Test">
		<classes>
			<class name="com.qa.opencart.test.LoginPageTest" />
		</classes>
	</test>

	<test name="Open Cart Accounts Page Test">
		<classes>
			<class name="com.qa.opencart.test.AccountsPageTest" />
		</classes>
	</test>

	<!-- <test name="Open Cart Product Info Page Test"> <classes> <class name="com.qa.opencart.tests.ProductInfoPageTest" 
		/> </classes> </test> <test name="Open Cart Registration Page Test"> <classes> 
		<class name="com.qa.opencart.tests.RegistrationPageTest" /> </classes> </test> -->

</suite>